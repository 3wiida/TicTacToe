<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="420.0" prefWidth="620.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="tic_tac_toe.view.popupgamestatus.PopUpGameController">
    <stylesheets>
        <URL value="@popupgame.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="439.0" fitWidth="655.0" layoutX="-10.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../assets/background.png" />
         </image>
      </ImageView>
      <Label fx:id="lblStatus" layoutX="127.0" layoutY="59.0" prefHeight="35.0" prefWidth="354.0" text="OOPS YOU LOSE!" />
      <Button fx:id="btnNewGame" layoutX="35.0" layoutY="336.0" mnemonicParsing="false" onAction="#newGameClicked" text="NEW GAME" />
      <Button fx:id="btnPlayAgain" layoutX="421.0" layoutY="336.0" mnemonicParsing="false" onAction="#playAgainClicked" text="PLAY AGAIN" />
      <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="400.0" layoutX="118.0" layoutY="116.0" />
   </children>
</AnchorPane>

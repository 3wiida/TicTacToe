<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="600.0" stylesheets="@loginorregisterpopup.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tic_tac_toe.view.popups.choose_login_signup.LoginOrRegisterPopupController">
   <children>
      <Label fx:id="asklbl" alignment="CENTER" layoutX="27.0" layoutY="62.0" prefHeight="38.0" prefWidth="546.0" text="Would you like to Login or Register ? " />
      <Button fx:id="registerBtn" layoutX="398.0" layoutY="132.0" mnemonicParsing="false" onAction="#onRegisterClicked" prefHeight="37.0" prefWidth="130.0" text="REGISTER" />
      <Button fx:id="loginBtn" layoutX="63.0" layoutY="132.0" mnemonicParsing="false" onAction="#onLoginClicked" prefHeight="37.0" prefWidth="130.0" text="LOGIN" />
      <ImageView fx:id="backPhoto" fitHeight="37.0" fitWidth="49.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#Onclick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/ic_back.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
